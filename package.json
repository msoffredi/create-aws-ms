{
    "name": "@msoffredi/create-aws-ms",
    "version": "0.1.0",
    "bin": "./build/index.js",
    "files": [
        "build/**/*"
    ],
    "type": "module",
    "scripts": {
        "clean": "rm -rf ./build",
        "build": "npm run clean && node_modules/typescript/bin/tsc && cp -r ./src/repo-template ./build",
        "build:local": "npm run build && chmod +x ./build/index.js",
        "test": "jest --runInBand --detectOpenHandles --forceExit --no-watchman",
        "test:coverage": "jest --runInBand --coverage",
        "test:watch": "jest --runInBand --watchAll --no-cache",
        "test:version": "jest --version"
    },
    "dependencies": {
        "cross-spawn": "^7.0.3",
        "inquirer": "^9.2.9",
        "yup": "^1.2.0"
    },
    "devDependencies": {
        "@tsconfig/node18": "^18.2.0",
        "@types/cross-spawn": "^6.0.2",
        "@types/inquirer": "^9.0.3",
        "@types/jest": "^29.5.3",
        "@types/node": "^18.17.1",
        "@typescript-eslint/eslint-plugin": "^6.2.1",
        "@typescript-eslint/parser": "^6.2.1",
        "eslint": "^8.46.0",
        "jest": "^29.6.2",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6"
    }
}
